  case 'linux-gnu' in \
    darwin[56]*) \
      need_charset_alias=true ;; \
    darwin* | cygwin* | mingw* | pw32* | cegcc*) \
      need_charset_alias=false ;; \
    *) \
      need_charset_alias=true ;; \
  esac ; \
else \
  need_charset_alias=false ; \
fi ; \
if $need_charset_alias; then \
  /bin/sh /builddir/build/BUILD/cpio-2.11/build-aux/install-sh -d /builddir/build/BUILDROOT/cpio-2.11-35.fc23.x86_64/usr/lib64 ; \
